{
  "version": 2,
  "framework": "vite",
  "buildCommand": "yarn install --frozen-lockfile && yarn build",
  "outputDirectory": "dist",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "functions": {
    "app/api/hello.js": {
      "runtime": "nodejs18.x"
    }
  },
  "env": {
    "YARN_ENABLE_IMMUTABLE_INSTALLS": "false"
  }
}
